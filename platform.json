{
  "name": "padauk",
  "title": "PADAUK PDK13 PDK14 PDK15 Microcontrollers",
  "description": "Padauk 8-Bit Microcontrollers",
  "homepage": "http://www.padauk.com.tw/en/product/index.aspx",
  "license": "Apache-2.0",
  "keywords": [
      "dev-platform",
      "8-bit"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TecMic/platformio-padauk"
  },
  "version": "0.0.3",
  "frameworks": {
    "easypdk": {
      "package": "framework-easypdk",
      "script": "builder/frameworks/easypdk.py"
    },
    "easypdk-hal": {
      "package": "framework-easypdk-hal",
      "script": "builder/frameworks/easypdk-hal.py"
    }
  },
  "packages": {
    "framework-easypdk": {
      "type": "framework",
      "optional": false,
      "version": "https://github.com/TecMic/framework-easypdk"
    },
    "framework-easypdk-hal": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/TecMic/framework-easypdk-hal"
    },
    "toolchain-sdcc": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "^1.40200.0"
    },
    "tool-easypdkprog": {
      "type": "uploader",
      "version": "https://github.com/TecMic/tool-easypdkprog"
    }
  }
}